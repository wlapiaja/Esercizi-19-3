// ES 1

var apply = function (array){
     return array[0](array[1]);

}

//ES 2

var aa = function(f){
  return function(array){
		return array.map(f);
	}
}

//ES 3

var comp2 ([f1, f2]){
	return function(x){
		return f1(f2(x));
	}
}

//ES 4

var distl = function (arr){
	var result = [];
	for (int i = 1; i < arr.length, i++){
		var coppia = [arr[0], arr[i]];
		result[i-1] = coppia;
	}
	return result;
}